#include <bits/stdc++.h>
using namespace std;

int sansaXor(vector<int> &arr) {
    int n = arr.size();
    int ans = 0;
    if (n % 2 == 0) return 0;
    for (int i = 0; i < n; i += 2) {
        ans ^= arr[i];
    }
    return ans;
}
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<int> arr(n);
        for (int i = 0; i < n; i++) cin >> arr[i];
        cout << sansaXor(arr) << "\n";
    }
    return 0;
}
